   DESTINATION share/catkin/
   USE_SOURCE_PERMISSIONS
+  PATTERN "*.orig" EXCLUDE
   )
 install(FILES
        ${CMAKE_CURRENT_BINARY_DIR}${CMAKE_FILES_DIRECTORY}/tests.cmake
